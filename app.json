{
  "expo": {
    "name": "BingOlympiades",
    "slug": "BingOlympiades",
    "extra": {
      "ADMIN_EMAIL": "admin@bingolympiades.com",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ceb5673c-65c5-4bc3-b5db-65f9047e38af"
      }
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/BingOlympiadesLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/BingOlympiadesLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cecelecrustace.BingOlympiades"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/BingOlympiadesLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/BingOlympiadesLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
